{
  "name": "zvid",
  "version": "0.1.8",
  "module": "./dist/index.mjs",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "start": "ts-node ./src/script.ts",
    "ts-check": "tsc --noEmit",
    "prepare": "husky",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": "prettier --write"
  },
  "keywords": [
    "ffmpeg",
    "nodejs",
    "video",
    "json",
    "rendering"
  ],
  "author": "Mina Ramzy",
  "license": "SEE LICENSE IN LICENSE",
  "description": "A node.js npm package for generating videos from JSON",
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.25",
    "@types/mime-types": "^3.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.4",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@napi-rs/canvas": "^0.1.77",
    "axios": "^1.7.7",
    "csstype": "^3.1.3",
    "file-type": "^21.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "mime-types": "^3.0.2",
    "puppeteer": "^23.2.0",
    "sharp": "^0.33.5"
  }
}
